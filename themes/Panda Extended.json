{
  "$schema": "vscode://schemas/color-theme",
  "comment": "Panda, Created by Siamak Mokhtari.",
  "author": "Siamak Mokhtari",
  "name": "Panda Theme Extended",
  "tokenColors": [
    {
      "settings": {
        "background": "#373b41",
        "foreground": "#f3f3f3"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#676B79",
        "fontStyle": " italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#19f9d8"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#7FD7FF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9AC1"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FF75B5"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#FFC990",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "   underline ",
        "foreground": "#FFC990"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#7FD7FF"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#CDCDCD",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF2C6D"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#7FD7FF"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "#FFC990"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF2C6D"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "",
        "foreground": "#CDCDCD"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "",
        "foreground": "#757575",
        "fontStyle": "  italic underline"
      }
    },
    {
      "name": "Italic HTML attribute names",
      "scope": "\n\t\t\t\t, constant.other.symbol.ruby,\n\t\t\t\t, entity.other.attribute-name.jsx,\n\t\t\t",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": "source meta.property-list meta.property-name support.type.property-name",
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "",
      "scope": "source meta.property-list meta.property-value keyword.other.unit",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "source.js keyword.operator.js",
      "scope": "source keyword.operator",
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "",
      "scope": "source.js entity.name.type.object.js.firebug",
      "settings": {
        "foreground": "#FFC990"
      }
    },
    {
      "name": "",
      "scope": "source meta.class.instance.constructor keyword.operator.new",
      "settings": {
        "fontStyle": " italic",
        "background": "",
        "foreground": "#FF75B5"
      }
    },
    {
      "name": "",
      "scope": "source.js meta.class.instance.constructor entity.name.type.instance.js",
      "settings": {
        "foreground": "#FFC990"
      }
    },
    {
      "name": "",
      "scope": "source meta.at-rule.media",
      "settings": {
        "foreground": "#B084EB"
      }
    },
    {
      "name": "",
      "scope": "source meta.at-rule.import variable.parameter.url",
      "settings": {
        "foreground": "#19f9d8"
      }
    },
    {
      "name": "",
      "scope": "source.sass variable.parameter.sass",
      "settings": {
        "foreground": "#FF9AC1"
      }
    },
    {
      "name": "",
      "scope": "source meta.property-value.css constant.other",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "",
      "scope": "source .property-value.css parameter constant.numeric.css keyword.other.unit.css",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": "meta.source.handlebars entity.name.tag",
      "settings": {
        "foreground": "#7FD7FF"
      }
    },
    {
      "scope": "punctuation.definition.expression, punctuation.definition.subexpression, punctuation.definition.block.unescaped, punctuation.definition.tag",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFC990"
      }
    },
    {
      "scope": "entity.name.function.expression",
      "settings": {
        "foreground": "#FF75B5"
      }
    },
    {
      "scope": "entity.unescaped.expression",
      "settings": {
        "foreground": "#B084EB"
      }
    },
    {
      "scope": "entity.expression variable.parameter.name",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": "entity.expression variable.parameter.value",
      "settings": {
        "foreground": "#7FD7FF"
      }
    },
    {
      "scope": "entity.expression support.function.builtin",
      "settings": {
        "foreground": "#FF9AC1"
      }
    },
    {
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#FFC990"
      }
    },
    {
      "scope": "entity.other.attribute-name.handlebars",
      "settings": {
        "foreground": "#FFC990"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#19f9d8"
      }
    },
    {
      "scope": "punctuation.definition.heading",
      "settings": {
        "foreground": "#19f9d8"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#FF75B5"
      }
    },
    {
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#CDCDCD"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "scope": "entity.name.type.instance.jsdoc punctuation.definition",
      "settings": {
        "foreground": "#FFC990",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#CDCDCD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.block storage",
      "settings": {
        "foreground": "#FFC990"
      }
    },
    {
      "scope": "comment.block storage.custom",
      "settings": {
        "foreground": "#FF75B5"
      }
    }
  ],
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.sublime_panda_syntax",
  "colors": {
    "foreground": "#e1e1e1",
    "activityBar.background": "#373b41",
    "activityBar.foreground": "#19f9d8",
    "activityBarBadge.background": "#7FD7FF",
    "editor.background": "#292A2B",
    "editor.foreground": "#e1e1e1",
    "editor.selectionBackground": "#4E5260",
    "sideBar.background": "#292A2B",
    "scrollbarSlider.activeBackground": "#19f9d880",
    "scrollbarSlider.hoverBackground": "#19f9d880",
    "statusBar.background": "#373b41",
    "tab.activeBackground": "#19f9d880"
  }
}