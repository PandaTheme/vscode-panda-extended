    {
      "scope": "keyword.operator.logical, keyword.operator.comparison",
      "settings": {
        "foreground": "#FFCC95"
      }
    },
    {
      "scope": "keyword.operator.assignment, keyword.operator.arithmetic",
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#FF75B5",
